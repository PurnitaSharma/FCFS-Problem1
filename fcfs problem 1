# FCFS-Problem1
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int p1,p2,p3,p4;
    scanf("%d%d%d%d",&p1,&p2,&p3,&p4);
    printf("P1 (WT=0), P2 (WT=%d), P3 (WT=%d), P4 (WT=%d)",p1,p1+p2,p1+p2+p3);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
